{
  "name": "ionic-es6-boilerplate",
  "version": "0.0.0",
  "description": "ionic-es6-boilerplate",
  "private": true,
  "author": {
    "name": "Marco Turi",
    "url": "https://github.com/marcoturi/ionic-es6-boilerplate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcoturi/ionic-es6-boilerplate"
  },
  "keywords": [
    "ionic",
    "boilerplate",
    "es6"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/eslint --cache .",
    "ios": "gulp serve --env=dev --api=prod --platform=ios",
    "android": "gulp serve --env=dev --api=prod --platform=ios",
    "web": "gulp serve --env=dev --api=prod --platform=web",
    "web-stage": "gulp serve --env=dev --api=stage --platform=web",
    "build": "gulp build --env=dev --api=prod",
    "build-stage": "gulp build --env=dev --api=stage",
    "build-release": "gulp build --env=prod --api=prod",
    "fast": "gulp serve --env=dev --api=prod --platform=web --nochk=true"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.0",
    "bower": "^1.3.3",
    "concat": "^2.0.0",
    "concurrently": "^2.0.0",
    "del": "^2.2.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.0.0",
    "eslint-plugin-react": "^4.1.0",
    "gulp": "^3.5.6",
    "gulp-angular-templatecache": "^1.8.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-bytediff": "^1.0.0",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.2.0",
    "gulp-if": "^2.0.0",
    "gulp-inject": "^3.0.0",
    "gulp-minify-css": "^0.3.0",
    "gulp-minify-html": "^1.0.6",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-remove-html": "^1.1.2",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rev": "^7.0.0",
    "gulp-sass": "^2.0.4",
    "gulp-shell": "^0.5.2",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-usemin": "^0.3.21",
    "gulp-util": "^2.2.20",
    "jasmine-async-sugar": "^1.3.3",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.3",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-story-reporter": "^0.3.1",
    "phantomjs": "^2.1.3",
    "phantomjs-prebuilt": "^2.1.6",
    "pre-commit": "^1.1.2",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "shelljs": "^0.3.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard",
    "cordova-plugin-splashscreen"
  ],
  "cordovaPlatforms": [
    "ios"
  ],
  "license": "GPL-3.0"
}
